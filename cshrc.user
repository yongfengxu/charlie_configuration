# ----------------------------------------------------------------------------
# Name     : .cshrc.user
# Function : Users private startup-file for csh and tcsh in the ARCenviroment
#
# Note     : Please do not edit this file until you have read the
#            site policy file for dot-files: /etc/home/README

#
# ----------------------------------------------------------------------------
#set path=( /opt/quest/bin $path /opt/sfw/bin )
#
# CCASE_BLD_UMASK 007
# CLEARCASE_RMVIEW_CR_FIX 1
# backed out 2012-05-23 #module add sudo/1.6.8p12_pam
umask 007


alias vim vimx
alias percol ~/projs/bin/percol
alias ee ~/projs/bin/emacs
alias kk ~/projs/bin/kk
alias gtags ~/projs/bin/gtags
alias pp ~/projs/bin/python3.6
alias pip ~/.local/bin/pip
alias mm ~/projs/bin/meld
alias ccmake ~/projs/bin/cmake
alias lg /lab/epg_ft_utils/logviewer/logviewer.sh
alias ww /lab/j20/testtools/binaries/rhel6.x86_64/onewireshark/bin/wireshark
alias p5g git push origin HEAD:refs/for/prototype/5g_poc_phase3
alias p2b git push origin HEAD:refs/for/prototype/wp390_281
alias p2m git push origin HEAD:refs/for/master
alias lncc ln -s build/Linux_x86_64/compile_commands.json compile_commands.json
alias get_maintainer /proj/epg/tools/bin/get_maintainer.py
alias jj jenkins_job build_all -n vipp,test -t -l "TTCN-BASIC-EVRTD_cups,TTCN-EXTENDED-EVRTD_cups,TTCN-CUPS_T2_BASIC-EVRTD_cups"

setenv ASAN_OPTIONS detect_stack_use_after_scope=0

#============================
#set danymic lib
#============================
setenv LD_LIBRARY_PATH /home/elmpprv/projs/lib64/:/home/elmpprv/projs/lib/:/usr/local/lib/:/usr/local/lib64/

#============================
#set head path
#============================
setenv C_INCLUDE_PATH /home/elmpprv/projs/include/
setenv CPLUS_INCLUDE_PATH /home/elmpprv/projs/include/
setenv PATH /home/elmpprv/projs/bin/:/home/elmpprv/.local/bin/:$PATH/

#============================
#install guile
#============================
setenv BDW_GC_CFLAGS /home/elmpprv/projs/include/gc.h
setenv BDW_GC_LIBS /home/elmpprv/projs/lib/libgc.so
setenv LIBFFI_CFLAGS /home/elmpprv/projs/lib/libffi-3.2.1/include/ffi.h
setenv LIBFFI_LIBS /home/elmpprv/projs/lib64/
#============================

#============================
# load meld
#============================
module load meld/1.8.4
module rm pygtk/2.24.0-2.7
#============================

setenv PKG_CONFIG_PATH /home/elmpprv/projs/bin/
#setenv PYTHONPATH /home/elmpprv/projs/lib/python3.6/site-packages/
setenv PYTHONPATH ~/projs/lib/python2.7/site-packages/
setenv GRAPHVIZ_DOT ~/projs/bin/dot

setenv EPG_GDB /repo/elmpprv/epg_gdb/epg_gdb.py
#alias compile_ttcn $WS_ROOT/ttcn3/scripts/official/compile_ttcn.sh build
#alias copy_ttcn $WS_ROOT/ttcn3/scripts/official/copy_ttcn3.sh
#
alias epg cd /repo/elmpprv/epg
alias sw setup_workspace
alias jp java -jar /home/elmpprv/plantuml.jar -graphvizdot ~/projs/bin/dot
